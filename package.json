{
  "name": "heloc-accelerator",
  "version": "0.1.0",
  "description": "HELOC Accelerator - Financial calculator running on port 3001",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --port 3001",
    "dev:fast": "node scripts/dev-fast.js",
    "dev:open": "next dev --turbo --port 3001 && open http://localhost:3001",
    "dev:domain": "next dev --turbo --port 3001 --hostname heloc-dev.local",
    "dev:domain:open": "next dev --turbo --port 3001 --hostname heloc-dev.local && open http://heloc-dev.local:3001",
    "dev:domain:https": "next dev --turbo --port 3001 --hostname heloc-dev.local --experimental-https",
    "dev:info": "node scripts/dev-info.js",
    "domain:setup": "node scripts/setup-local-domain.js",
    "domain:remove": "node scripts/remove-local-domain.js",
    "https:setup": "node scripts/setup-https.js",
    "ngrok:setup": "node scripts/setup-ngrok.js",
    "tunnel": "ngrok http 3001",
    "build": "next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "npm run analyze",
    "db:setup": "./scripts/db-setup.sh",
    "db:setup:docker": "./scripts/db-setup.sh docker",
    "db:setup:native": "./scripts/db-setup.sh native",
    "db:status": "./scripts/db-sync.sh status",
    "db:backup": "./scripts/db-sync.sh backup-local",
    "db:sync:to-cloud": "./scripts/db-sync.sh sync-to-cloud",
    "db:sync:from-cloud": "./scripts/db-sync.sh sync-from-cloud",
    "db:create-user": "node scripts/create-test-user.js",
    "db:cli": "node scripts/db-cli.js",
    "db:users": "node scripts/db-cli.js list-users",
    "db:migrate": "node scripts/db-cli.js migrate",
    "db:neon:setup": "node scripts/setup-neon-db.js",
    "db:neon:test": "node scripts/test-neon-connection.js",
    "db:neon:import-users": "node scripts/import-test-users.js",
    "db:neon:test-auth": "node scripts/test-database-auth.js",
    "seed:demo-assignments": "node scripts/seed-demo-assignments.js",
    "check:imports": "node scripts/validate-imports.js",
    "check:deploy": "node scripts/pre-deploy-check.js",
    "check:vercel": "node scripts/vercel-check.js",
    "check:contrast": "node scripts/contrast-check.js",
    "check:tokens": "node scripts/check-tokens.js",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "force-deploy": "./scripts/force-vercel-deploy.sh",
    "vercel:force": "./scripts/force-vercel-deploy.sh",
    "quick-deploy": "./scripts/quick-deploy.sh",
    "prepare": "husky install || true",
    "augment:pre-edit": "node .augment/automation/quality-framework.js pre-edit",
    "augment:post-edit": "node .augment/automation/quality-framework.js post-edit",
    "augment:ui-check": "node .augment/automation/quality-framework.js ui-check",
    "augment:session": "node .augment/automation/quality-framework.js session-status",
    "augment:workflow:pre": "node .augment/automation/task-workflows.js pre-edit",
    "augment:workflow:post": "node .augment/automation/task-workflows.js post-edit",
    "augment:workflow:ui": "node .augment/automation/task-workflows.js ui",
    "augment:workflow:deploy": "node .augment/automation/task-workflows.js deployment",
    "test:stack-auth": "node test-stack-auth.js",
    "docker:setup": "./docker-setup.sh setup",
    "docker:setup:prod": "./docker-setup.sh setup-prod",
    "docker:start": "./docker-setup.sh start",
    "docker:start:dev": "./docker-setup.sh start-dev",
    "docker:stop": "./docker-setup.sh stop",
    "docker:status": "./docker-setup.sh status",
    "docker:logs": "./docker-setup.sh logs",
    "docker:logs:app": "./docker-setup.sh logs app",
    "docker:cleanup": "./docker-setup.sh cleanup"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@sentry/nextjs": "^10.8.0",
    "@stackframe/stack": "^2.8.25",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "eslint": "^8",
    "eslint-config-next": "^15.5.2",
    "framer-motion": "^12.23.11",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.534.0",
    "next": "^15.5.2",
    "next-auth": "^5.0.0-beta.29",
    "next-intl": "^4.3.4",
    "pg": "^8.11.3",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.61.1",
    "recharts": "^2.12.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@axe-core/react": "^4.10.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/jest": "^29.5.12",
    "@types/jest-axe": "^3.5.9",
    "axe-core": "^4.10.2",
    "dotenv": "^17.2.1",
    "eslint-plugin-contrast-validation": "file:./eslint-rules",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2"
  }
}
