{
  "mcpServers": {
    "serena": {
      "command": "serena-mcp",
      "args": [],
      "env": {}
    },
    "context7": {
      "command": "context7-mcp",
      "args": [],
      "env": {}
    },
    "playwright": {
      "command": "playwright-mcp",
      "args": [],
      "env": {}
    },
    "sequential-thinking": {
      "command": "sequential-thinking-mcp",
      "args": [],
      "env": {}
    },
    "browser-tools": {
      "command": "npx",
      "args": ["@agentdeskai/browser-tools-mcp@1.2.0"],
      "env": {}
    },
    "supabase": {
      "command": "supabase-mcp",
      "args": [],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_KEY": "${SUPABASE_KEY}"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${DATABASE_URL}"
      ],
      "env": {}
    },
    "neon": {
      "command": "npx",
      "args": ["-y", "@neondatabase/mcp-server-neon", "start"],
      "env": {
        "NEON_API_KEY": "${NEON_API_KEY}"
      }
    },
    "chrome-mcp-stdio": {
      "command": "node",
      "args": [
        "/Users/elliotnoteware/.npm-global/lib/node_modules/mcp-chrome-bridge/dist/mcp/mcp-server-stdio.js"
      ],
      "env": {}
    },
    "chrome-mcp-server": {
      "type": "streamableHttp",
      "url": "http://127.0.0.1:12306/mcp"
    }
  }
}
